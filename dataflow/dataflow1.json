{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "CleanedCompaniesCSV",
						"type": "DatasetReference"
					},
					"name": "cleanedCompanyCSV"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "AzureSqlTable1",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "RevenueClean",
					"description": "RevenueClean"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> cleanedCompanyCSV",
				"cleanedCompanyCSV derive(RevenueClean = toDouble(replace(replace(Revenue, '$', ''), ',', ''))) ~> RevenueClean",
				"RevenueClean sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Rank as integer,",
				"          Name as string,",
				"          Industry as string,",
				"          Country as string,",
				"          Revenue as double,",
				"          Employees as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          Revenue = RevenueClean",
				"     )) ~> sink1"
			]
		}
	}
}